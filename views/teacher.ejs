<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" type="text/css" href="/css/style.css"> 
  <title>Teachers List</title>
</head>
<body>
    <h1>TEACHER'S LIST</h1>
    <p>Let's back <a href="/">HOME</a></p>
    <table>
      <tr>
        <th>id</th>
        <th>Teacher</th>
      </tr>
      <% teacherList.forEach(eachTeacher => {  %>
        <tr>
            <% Object.keys(eachTeacher).forEach(data => { %>
            <td><%= eachTeacher[data]%></td>
            <% }) %>
        </tr>
      <%}) %>
    </table>
</body>
</html>